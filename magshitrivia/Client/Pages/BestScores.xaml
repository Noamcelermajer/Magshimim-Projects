<Window x:Class="Client.BestScores"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="BestScores" Height="450" Width="800" Background="LightSlateGray">
    <Grid Margin="-2,0,2,-1">
        <Grid.Background>
            <ImageBrush ImageSource="background.png"
                AlignmentY="Center" AlignmentX="Center" Stretch="Fill"/>
        </Grid.Background>
        <Button local:CornerRadiusSetter.CornerRadius="12" Content="Back To Menu" Margin="336,348,336,23" VerticalAlignment="Center" Name ="BackToMenu" Height="49" BorderThickness="0" Background="White" Foreground="Black" FontSize="16" FontFamily="MV Boli" Click="Back_Click"/>
        <Label Content="Label" Name="Score" Height="258" Margin="10,0,10,77" VerticalAlignment="Bottom" FontFamily="Levenim MT" FontSize="18" Background="White" Foreground="Black"/>
    </Grid>
</Window>
